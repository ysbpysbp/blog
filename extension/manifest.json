{
  "manifest_version": 2,
  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script/content_script.js"
      ],
      "matches": [ "http://*/*", "https://*/*" ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*.mttxe.com/",
    "tabs"
  ]
}
